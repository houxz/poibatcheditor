<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<bean id="menuConfig" class="cn.emg.poibatcheditor.config.MenuConfig">
		<property name="menus">
			<set>
				<ref bean="edit" />
			</set>
		</property>
	</bean>

	<bean id="edit" class="cn.emg.poibatcheditor.auth.MenuAuthModel">
		<property name="code" value="1001" />
		<property name="url" value="/poibatcheditor/edit.web" />
		<property name="label" value="制作" />
		<property name="roleSet">
			<set>
				<value type="cn.emg.poibatcheditor.commonjar.RoleTypeEnum">ROLE_WORKER</value>
			</set>
		</property>
		<property name="enabled" value="true" />
		<property name="isshow" value="true" />
	</bean>
	
</beans>